---
layout: null
---
[
  {% for post in site.posts %}
    {
      "title"    : "{{ post.title | escape }}",
      "categories" : "{{ post.categories  | join: ', '   }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date }}"
    },
  {% endfor %}
  {% for post in site.robotica %}
  {
    "title"    : "{{ post.title | escape }}",
    "categories" : "{{ post.categories  | join: ', '   }}",
    "tags"     : "{{ post.tags | join: ', ' }}",
    "url"      : "{{ site.baseurl }}{{ post.url }}",
    "date"     : "{{ post.date }}"
  } {% unless forloop.last %},{% endunless %}
{% endfor %}

]
